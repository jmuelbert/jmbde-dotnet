{
  "version": "0.0.2-*",
  "description": "Application with Database for BDE",
  "repository": {
      "type": "git",
      "url": "https://github.com/jmuelbert/jmbde-aspnet"
  },
  "compilationOptions": {
    "emitEntryPoint": true,
    "xmlDoc": true
  },
  "tooling": {
    "defaultNamespace": "jmbde"
  },

  "dependencies": {
    "EntityFramework.Commands": "7.0.0-*",
    "EntityFramework.SQLite": "7.0.0-*",
    "EntityFramework.Sqlite.Design": "7.0.0-*",
    "EntityFramework.MicrosoftSqlServer": "7.0.0-*",
    "EntityFramework.MicrosoftSqlServer.Design": "7.0.0-*",
    "Microsoft.AspNet.Diagnostics": "1.0.0-*",
    "Microsoft.AspNet.Identity.EntityFramework": "3.0.0-*",
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-*",
    "Microsoft.AspNet.Mvc": "6.0.0-*",
    "Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-*",
    "Microsoft.AspNet.Localization": "1.0.0-*",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-*",
    "Microsoft.AspNet.StaticFiles": "1.0.0-*",
    "Microsoft.AspNet.Tooling.Razor": "1.0.0-*",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0-*",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0-*",
    "Microsoft.Extensions.Configuration.FileProviderExtensions" : "1.0.0-*",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-*",
    "Microsoft.Extensions.Configuration.Ini": "1.0.0-*",
    "Microsoft.Extensions.Localization.Abstractions": "1.0.0-*",
    "Microsoft.Extensions.Logging": "1.0.0-rc1-final",
    "Microsoft.Extensions.Logging.Console": "1.0.0-*",
    "Microsoft.Extensions.Logging.Debug": "1.0.0-*"
  },

  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel",
    "ef": "EntityFramework.Commands"
  },

  "frameworks": {
    "dnx451": {},
    "dnxcore50": {}
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "scripts": {
    "prepublish": [
      "npm install",
      "bower install",
      "gulp clean",
      "gulp min"
    ]
  }
}
